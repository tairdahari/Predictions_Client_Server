<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane fitToHeight="true" fitToWidth="true" pannable="true" prefHeight="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="subComponents.mainScreen.app.AppController">
   <content>
      <BorderPane>
         <top>
            <fx:include fx:id="headerComponent" source="../header/header.fxml" BorderPane.alignment="TOP_LEFT" />
         </top>
         <center>
            <AnchorPane prefHeight="447.0" BorderPane.alignment="CENTER">
               <children>
                  <fx:include fx:id="bodyComponent" source="../body/body.fxml" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" BorderPane.alignment="TOP_LEFT" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </content>
</ScrollPane>
